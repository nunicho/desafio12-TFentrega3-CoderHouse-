
<h2>CONTROL DE AUTENTICACIÓN</h2>

{{#if usuario}}
  <div>Está logueado el usuario:</div>
  <ul>
    {{#each usuario}}
      <li>{{@key}}: {{this}}</li>
    {{/each}}
  </ul>
{{else}}
  <div>No hay usuario autenticado</div>
{{/if}}

<br>
{{>menu}}


